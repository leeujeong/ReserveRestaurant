<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.reserve.restaurant.repository.RestaurantRepository">
	
	<insert id="insertUser" parameterType="com.reserve.restaurant.domain.User">
		INSERT INTO USERTABLE
		VALUES(USERTABLE_SEQ.NEXTVAL, #{id}, #{pw}, #{user_name}, #{user_tel}, #{user_grade},#{user_date},#{user_hbd},#{state},#{point},#{user_email} )
	</insert>
	
</mapper>